generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int       @id @default(autoincrement())
  email     String    @unique
  name      String
  champions Champion[]
}

model Champion {
  id        Int     @id @default(autoincrement())
  name      String
  rating    Int?
  comment   String?
  userId    Int
  user      User    @relation(fields: [userId], references: [id])
}
